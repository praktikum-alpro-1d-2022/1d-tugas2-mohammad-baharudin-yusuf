#include <iostream>
using namespace std;

int main(){
 
      //Menghitung luas persegi panjang
      float p2, l2, L2;
      cout << "Masukan nilai panjang dan lebar dari persegi panjang : \n";
      cin >> p2 >> l2;
      L2 = p2*l2;
      cout << "Hasil hitung luas persegi panjang adalah : "<<L2;
      cin.get();
      
      return 0;
      
}
      
